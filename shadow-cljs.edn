{:source-paths
 ["src/dev"
  "src/main"
  "src/test"]

 :dependencies
 [[reagent "1.1.1"]
  [binaryage/oops "0.7.2"]
  [binaryage/devtools "1.0.6"]
  [org.clojure/test.check "1.1.1"]
  [cider/cider-nrepl "0.28.5"]
  [cljs-bean "1.8.0"]]

 :builds
 {:han
  {:target :react-native
   :init-fn app/init
   :output-dir "build"
   :devtools {:autoload true
              :preloads [devtools.preload]}
   :compiler-options {:infer-externs :auto}}

  :player
  {:target :browser
   :asset-path "/audio-engine"
   :output-dir "public/audio-engine"
   :devtools {:loader-mode :eval
              :console-support false}
   :modules {:index {:entries [audio.engine]}}
   :compiler-options {:infer-externs :auto}}

  :test
  {:target :node-test
   :output-to "build/test.js"
   :ns-regexp "-test$"
   :autorun   true
   :compiler-options {:reader-features #{:test}}}}

 :nrepl {:port 9000}}
